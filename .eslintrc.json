{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "key-spacing": [
            2,
            {
                "afterColon": true,
                "mode": "minimum"
            }
        ],
        "new-cap": [
            2,
            {
                "newIsCap": true
            }
        ],
        "new-parens": 2,
        "newline-after-var": [
            2,
            "always"
        ],
        "no-lonely-if": 2,
        "no-mixed-spaces-and-tabs": [
            2,
            "smart-tabs"
        ],
        "no-trailing-spaces": 2,
        "no-unneeded-ternary": 2,
        "comma-dangle": [
            2,
            "never"
        ],
        "no-console": 1,
        "no-constant-condition": 2,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [
            2,
            "all"
        ],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-regex-spaces": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "no-unexpected-multiline": 2,
        "block-scoped-var": 2,
        "consistent-return": 2,
        "curly": [
            2,
            "all"
        ],
        "default-case": 2,
        "dot-notation": 2,
        "dot-location": [
            2,
            "property"
        ],
        "eqeqeq": [
            2,
            "allow-null"
        ],
        "no-alert": 1,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extra-bind": 2,
        "no-implicit-coercion": [
            2,
            {
                "boolean": true,
                "number": true,
                "string": true
            }
        ],
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-param-reassign": [
            2,
            {
                "props": true
            }
        ],
        "no-process-env": 2,
        "no-proto": 2,
        "no-redeclare": [
            2,
            {
                "builtinGlobals": true
            }
        ],
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-void": 2,
        "no-with": 2,
        "radix": 2,
        "wrap-iife": [
            2,
            "any"
        ],
        "yoda": [
            2,
            "never",
            {
                "exceptRange": true
            }
        ],
        "strict": 2,
        "no-catch-shadow": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-undef-init": 2,
        "no-undef": 2,
        "no-undefined": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "callback-return": 1,
        "handle-callback-err": 1,
        "no-mixed-requires": [
            2,
            false
        ],
        "no-new-require": 2,
        "no-process-exit": 1,
        "array-bracket-spacing": [
            2,
            "never",
            {}
        ],
        "brace-style": [
            2,
            "1tbs",
            {
                "allowSingleLine": true
            }
        ],
        "camelcase": [
            2,
            {
                "properties": "always"
            }
        ],
        "comma-spacing": [
            2,
            {
                "after": true
            }
        ],
        "computed-property-spacing": [
            2,
            "never"
        ],
        "eol-last": 2,
        "indent": [
            2,
            "tab"
        ],
        "lines-around-comment": [
            2,
            {
                "beforeBlockComment": true,
                "afterBlockComment": false,
                "beforeLineComment": true,
                "allowBlockStart": true,
                "allowBlockEnd": true
            }
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi-spacing": [
            2,
            {
                "after": true
            }
        ],
        "sort-vars": [
            2,
            {
                "ignoreCase": true
            }
        ],
        "keyword-spacing": [
            2,
            {
                "after": true,
                "overrides": {
                    "if": {
                        "after": false
                    },
                    "for": {
                        "after": false
                    },
                    "while": {
                        "after": false
                    },
                    "catch": {
                        "after": false
                    }
                }
            }
        ],
        "space-before-blocks": [
            2,
            "always"
        ],
        "space-before-function-paren": [
            2,
            "never"
        ],
        "space-infix-ops": 2,
        "spaced-comment": [
            2,
            "never"
        ],
        "arrow-parens": [
            2,
            "always"
        ],
        "arrow-spacing": [
            2,
            {
                "before": true,
                "after": true
            }
        ],
        "constructor-super": 1,
        "generator-star-spacing": 2,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-this-before-super": 2,
        "prefer-const": 1,
        "prefer-spread": 1,
        "quotes": [
            2,
            "single"
        ]
    }
}
